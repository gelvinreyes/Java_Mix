import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        try{
            Scanner in = new Scanner(System.in);
            int x = in.nextInt();
            int y = in .nextInt();
            long result=x/y;
            System.out.println(result);
        }
        catch(Exception ex){
            String[] e= ex.toString().split(":");
            if(e.length>1 && e[1].contains("zero"))
            {
                System.out.println(ex);            
            }
            else
            {
                System.out.println(e[0]);
            }
        }
    }
}